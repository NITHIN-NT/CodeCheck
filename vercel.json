{
    "version": 2,
    "builds": [
      {
        "src": "review_dashboard/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "review_dashboard/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "review_dashboard.settings",
      "PYTHONUNBUFFERED": "1"
    }
  }
  