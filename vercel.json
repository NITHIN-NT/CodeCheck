{
    "version": 2,
    "outputDirectory": "staticfiles",
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "review_dashboard/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "review_dashboard.settings",
      "PYTHONUNBUFFERED": "1"
    }
  }
  